{
    "service": {
        "name": "carbon",
        "port": 2003,
        "check": {
            "name": "Carbon-Cache Check",
            "notes": "Connect to the port internally every 10 seconds",
            "script": "nc -z localhost 2003 >/dev/null 2>&1",
            "interval": "10s"
        }
    }
}
